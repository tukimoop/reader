{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "akaunting/setting": "^1.0",
        "akaunting/version": "^1.0",
        "beyondcode/laravel-self-diagnosis": "^0.4.1",
        "caffeinated/modules": "^4.3",
        "cloudflare/sdk": "^1.1",
        "fideloper/proxy": "^4.0",
        "intervention/imagecache": "^2.3",
        "laracasts/flash": "^3.0",
        "laracasts/generators": "^1.1",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "laravolt/avatar": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "lida-io/discord-webhooks": "^0.3.1",
        "owen-it/laravel-auditing": "^7.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.21.0",
        "silber/bouncer": "v1.0.0-rc.1",
        "symfony/intl": "^4.1",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
